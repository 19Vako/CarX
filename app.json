{
  "expo": {
    "name": "carX",
    "slug": "carX",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "carX",
    "platforms": ["ios", "android", "web"],
    "userInterfaceStyle": "automatic",
    "backgroundColor": "#808080",
    "androidStatusBar": {
      "hidden": true,
      "translucent": true,
      "backgroundColor": "#808080"
    },
    "androidNavigationBar": {
      "visible": "leanback",
      "barStyle": "dark-content",
      "backgroundColor": "#808080"
    },

    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sergii.carX",
      "googleServicesFile": "./GoogleService-Info.plist",
      "scheme": "com.sergii.carX",
      "icon": "./assets/images/favicon.png",
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "carX",
              "com.sergii.carX",
              "com.googleusercontent.apps.745026637842-srtjnigkf7ipbkqj66qcrd7c9pml7gr1"
            ]
          }
        ],

        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "permissions": ["ACCESS_FINE_LOCATION", "ACCESS_COARSE_LOCATION"],
      "googleServicesFile": "./google-services.json",
      "package": "com.sergii.carX",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/favicon.png",
        "backgroundColor": "#808080"
      },
      "edgeToEdgeEnabled": true,
      "manifestPlaceholders": {
        "appAuthRedirectScheme": "carX"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#808080"
        }
      ],
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.561248963797-ng585ieq3ael47icplmhdll0gbnl52p1"
        }
      ],
      "expo-secure-store",
      [
        "@sentry/react-native/expo",
        {
          "url": "https://sentry.io/",
          "project": "carx",
          "organization": "ggroup"
        }
      ],
      "@sentry/react-native",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "appAuthRedirectScheme": "carX",
      "eas": {
        "projectId": "a6094235-e94a-458c-8d0a-db766fdf822b"
      }
    },
    "owner": "19vako",
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "https://u.expo.dev/a6094235-e94a-458c-8d0a-db766fdf822b"
    }
  }
}
