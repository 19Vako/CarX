{
  "expo": {
    "name": "carX",
    "slug": "carX",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "carx",
    "userInterfaceStyle": "automatic",
    "backgroundColor": "gray",
    "androidStatusBar": {
      "hidden": true,
      "translucent": true,
      "backgroundColor": "gray"
    },
    "androidNavigationBar": {
      "visible": true
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sergii.carx",
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "com.googleusercontent.apps.792891989585-4ctetr4bq29skcl08fo814thvkd34vu5"
            ]
          }
        ]
      }
    },
    "android": {
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ],
      "package": "com.sergii.carx",
      "adaptiveIcon": {},
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "server",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "gray"
        }
      ],
      "expo-web-browser",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "a6094235-e94a-458c-8d0a-db766fdf822b"
      }
    }
  }
}
